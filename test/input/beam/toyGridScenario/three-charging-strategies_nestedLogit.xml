<?xml version="1.0" encoding="utf-8"?>
<strategies>
<strategy>
    <id>1</id>
    <name>NestedLogit</name>
    <className>ChargingStrategyNestedLogit</className>
    <parameters>
      <nestedLogit name="arrival">
         <elasticity>1</elasticity>
         <nestedLogit name="yesCharge">
           <elasticity>1</elasticity>
           <nestedLogit name="genericSitePlugTypeAlternative">
            <elasticity>0.5</elasticity>
            <utility>
               <param name="intercept" type="INTERCEPT">-12.0</param>
               <param name="cost" type="MULTIPLIER">-2</param>
               <param name="chargerCapacity" type="MULTIPLIER">0.001</param>
               <param name="distanceToActivity" type="MULTIPLIER">-1</param>
               <param name="remainingRange" type="MULTIPLIER">-2.5</param>
               <param name="remainingTravelDistanceInDay" type="MULTIPLIER">0.00</param>
               <param name="nextTripTravelDistance" type="MULTIPLIER">0.05</param>
               <param name="plannedDwellTime" type="MULTIPLIER">0.00</param>
               <param name="isHomeActivityAndHomeCharger" type="MULTIPLIER">5</param>
               <param name="isAvailable" type="MULTIPLIER">5.0</param>
               <param name="isBEV" type="MULTIPLIER">2.5</param>
            </utility>
           </nestedLogit>
         </nestedLogit>
         <nestedLogit name="noCharge">
            <elasticity>1</elasticity>
            <nestedLogit name="tryChargingLater">
               <elasticity>0.5</elasticity>
               <utility>
                 <param name="intercept" type="INTERCEPT">-2.5</param>
                 <param name="remainingRange" type="MULTIPLIER">-0.05</param>
                 <param name="remainingTravelDistanceInDay" type="MULTIPLIER">0.025</param>
                 <param name="nextTripTravelDistance" type="MULTIPLIER">0.05</param>
                 <param name="plannedDwellTime" type="MULTIPLIER">0.35</param>
                 <param name="isHomeActivity" type="MULTIPLIER">0.0</param>
                 <param name="isBEV" type="MULTIPLIER">-2.5</param>
                 <param name="searchRadius" type="MULTIPLIER">1.5</param>
               </utility>
            </nestedLogit>
            <nestedLogit name="continueSearchInLargerArea">
               <elasticity>0.5</elasticity>
               <utility>
                 <param name="intercept" type="INTERCEPT">-0.5</param>
                 <param name="remainingRange" type="MULTIPLIER">-0.05</param>
                 <param name="remainingTravelDistanceInDay" type="MULTIPLIER">0.025</param>
                 <param name="nextTripTravelDistance" type="MULTIPLIER">0.05</param>
                 <param name="plannedDwellTime" type="MULTIPLIER">0.35</param>
                 <param name="isHomeActivity" type="MULTIPLIER">-5.0</param>
                 <param name="isBEV" type="MULTIPLIER">2.5</param>
                 <param name="searchRadius" type="MULTIPLIER">-3.0</param>
               </utility>
            </nestedLogit>
            <nestedLogit name="abort">
               <elasticity>0.5</elasticity>
               <utility>
                 <param name="intercept" type="INTERCEPT">5.0</param>
                 <param name="remainingRange" type="MULTIPLIER">0.05</param>
                 <param name="remainingTravelDistanceInDay" type="MULTIPLIER">-0.025</param>
                 <param name="nextTripTravelDistance" type="MULTIPLIER">-0.05</param>
                 <param name="plannedDwellTime" type="MULTIPLIER">-0.35</param>
                 <param name="isHomeActivity" type="MULTIPLIER">2.0</param>
                 <param name="isBEV" type="MULTIPLIER">-2.5</param>
                 <param name="searchRadius" type="MULTIPLIER">1.0</param>
               </utility>
            </nestedLogit>
         </nestedLogit>
      </nestedLogit>
      <nestedLogit name="departure">
         <elasticity>1</elasticity>
         <nestedLogit name="yesCharge">
           <elasticity>1</elasticity>
           <nestedLogit name="genericSitePlugTypeAlternative">
            <elasticity>0.5</elasticity>
            <utility>
               <param name="intercept" type="INTERCEPT">0.5</param>
               <param name="cost" type="MULTIPLIER">-0.5</param>
               <param name="extraTimeRequired" type="MULTIPLIER">-0.001</param>
               <param name="extraEnergyRequired" type="MULTIPLIER">-0.001</param>
               <param name="chargerCapacity" type="MULTIPLIER">0.05</param>
               <param name="remainingRange" type="MULTIPLIER">-0.05</param>
               <param name="remainingTravelDistanceInDay" type="MULTIPLIER">0.025</param>
               <param name="nextTripTravelDistance" type="MULTIPLIER">0.05</param>
               <param name="isBEV" type="MULTIPLIER">2.5</param>
            </utility>
           </nestedLogit>
         </nestedLogit>
         <nestedLogit name="noCharge">
            <elasticity>1</elasticity>
             <utility>
               <param name="intercept" type="INTERCEPT">5.0</param>
               <param name="remainingRange" type="MULTIPLIER">0.05</param>
               <param name="remainingTravelDistanceInDay" type="MULTIPLIER">-0.025</param>
               <param name="nextTripTravelDistance" type="MULTIPLIER">-0.05</param>
               <param name="isBEV" type="MULTIPLIER">-2.5</param>
               <param name="searchRadius" type="MULTIPLIER">1.0</param>
             </utility>
          </nestedLogit>
      </nestedLogit>
    </parameters>
  </strategy>
  <strategy>
    <id>2</id>
    <name>AlwaysChargeOnArrival</name>
    <className>ChargingStrategyAlwaysChargeOnArrival</className>
    <parameters>
    </parameters>
  </strategy>
  <strategy>
    <id>3</id>
    <name>ChargeOnFirstDeparture</name>
    <className>ChargingStrategyChargeOnFirstDeparture</className>
    <parameters>
    </parameters>
  </strategy>
  <priorProbability>
    <conditional>
      <conditionedOn name="activityType">
        <Home>
          <strategy name="NestedLogit">1.0</strategy>
          <strategy name="AlwaysChargeOnArrival">1.0</strategy>
          <strategy name="ChargeOnFirstDeparture">1.0</strategy>
        </Home>
        <Work>
          <strategy name="NestedLogit">1.0</strategy>
          <strategy name="AlwaysChargeOnArrival">1.0</strategy>
          <strategy name="ChargeOnFirstDeparture">1.0</strategy>
        </Work>
        <Escort>
          <strategy name="NestedLogit">1.0</strategy>
          <strategy name="AlwaysChargeOnArrival">1.0</strategy>
          <strategy name="ChargeOnFirstDeparture">1.0</strategy>
        </Escort>
        <Eatout>
          <strategy name="NestedLogit">1.0</strategy>
          <strategy name="AlwaysChargeOnArrival">1.0</strategy>
          <strategy name="ChargeOnFirstDeparture">1.0</strategy>
        </Eatout>
        <Shopping>
          <strategy name="NestedLogit">1.0</strategy>
          <strategy name="AlwaysChargeOnArrival">1.0</strategy>
          <strategy name="ChargeOnFirstDeparture">1.0</strategy>
        </Shopping>
        <School>
          <strategy name="NestedLogit">1.0</strategy>
          <strategy name="AlwaysChargeOnArrival">1.0</strategy>
          <strategy name="ChargeOnFirstDeparture">1.0</strategy>
        </School>
        <University>
          <strategy name="NestedLogit">1.0</strategy>
          <strategy name="AlwaysChargeOnArrival">1.0</strategy>
          <strategy name="ChargeOnFirstDeparture">1.0</strategy>
        </University>
        <Other>
          <strategy name="NestedLogit">1.0</strategy>
          <strategy name="AlwaysChargeOnArrival">1.0</strategy>
          <strategy name="ChargeOnFirstDeparture">1.0</strategy>
        </Other>
      </conditionedOn>
    </conditional>
  </priorProbability>
</strategies>
